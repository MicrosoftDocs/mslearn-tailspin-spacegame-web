<Project>
  <PropertyGroup>
    <MinimumMSBuildVersion>17.11.0</MinimumMSBuildVersion>
    <BundledMSBuildVersion>17.12.18</BundledMSBuildVersion>
    <_MSBuildVersionMajorMinor>$([System.Version]::Parse('$(MSBuildVersion)').ToString(2))</_MSBuildVersionMajorMinor>
    <_IsDisjointMSBuildVersion>$([MSBuild]::VersionLessThan('$(_MSBuildVersionMajorMinor)', '17.12'))</_IsDisjointMSBuildVersion>
  </PropertyGroup>
</Project>
